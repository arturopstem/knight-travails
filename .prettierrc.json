{
  "singleQuote": true,
  "overrides": [
    {
      "files": [
        "*.css",
        "*.scss",
        "*.sass",
        "*.less"
      ],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}